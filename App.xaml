<Application x:Class="Skinning.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="DemoWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="skin" Source="pack://siteOfOrigin:,,,/Modes/ListMode.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <XmlDataProvider x:Key="XMLDataDataSource" Source="pack://siteOfOrigin:,,,/Data/XMLData.xml"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
